{
    "name": "sails-json-api",
    "version": "0.4.0",
    "description": "JSON API compatible controllers, policies, services and generators for Sails v1.0+",
    "main": "index.js",
    "scripts": {
        "start": "node app.js",
        "test": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/**/*.test.js --require @babel/register",
        "format": "node ./node_modules/.bin/prettier --single-quote --tab-width 2 --print-width 120 --trailing-comma none --write \"{{api,bin,config,test,lib}/**/*.js,index.js,app.js}\"",
        "cover": "nyc ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/**/*.test.js --require @babel/register"
    },
    "keywords": [
        "json-api",
        "jsonapi",
        "generator",
        "controller",
        "virtual",
        "sails",
        "generate",
        "plugin",
        "addon"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ajgribble/sails-json-api.git"
    },
    "homepage": "https://github.com/ajgribble/sails-json-api#readme",
    "author": "Adam Gribble",
    "license": "MIT",
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "json-api-serializer": "2.3.0",
        "lodash": "4.17.15",
        "pluralize": "8.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.8.4",
        "@babel/preset-env": "7.8.4",
        "@babel/register": "7.8.3",
        "async": "3.1.1",
        "babel-plugin-add-module-exports": "1.0.2",
        "chai": "4.2.0",
        "mocha": "7.0.1",
        "nyc": "15.0.0",
        "prettier": "1.19.1",
        "sails": "1.2.3",
        "sails-disk": "1.1.2",
        "sails-hook-orm": "2.1.1",
        "sails-hook-sockets": "2.0.0",
        "supertest": "4.0.2",
        "waterline": "0.13.6"
    },
    "sails": {
        "isHook": true
    }
}
